mixin profile-main()

  .profile-main
    form#profileData.profile-main__form(action="")
      fieldset.profile-main__header
        h2.profile-main__title
          | Основные данные

        .profile-main__inner
          .profile-main__input-group
            label.profile-main__label(for="mail")
              | Электронная почта
            input#mail.profile-main__input-mail(type="mail" placeholder="personal@hotkupon.ru" name="mail")
            button.profile-main__btn.btn
              svg.profile-main__mail-icon(width="17" height="13")
                use(xlink:href="img/sprite-svg.svg#modals__mail-icon")
              | Проверить почту

          .profile-main__input-group
            label.profile-main__label(for="name")
              | Имя пользователя
            input#name.profile-main__input(type="text" placeholder="testing name" name="name")

          .profile-main__input-group
            label.profile-main__label(for="login")
              | Логин
            input#login.profile-main__input(type="text" placeholder="harold1" name="login")

          .profile-main__input-group
            label.profile-main__label(for="pass")
              | Пароль
            input#pass.profile-main__input-pass(type="password" name="pass" placeholder="********")
            button.profile-main__btn.profile-main__btn--gray.btn
              | Сменить пароль

      fieldset.profile-main__body
        h2.profile-main__title
          | Личные данные

        .profile-main__inner
          .profile-main__lcol
            .profile-main__input-group
              label.profile-main__label(for="gender")
