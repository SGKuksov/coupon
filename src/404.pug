include mixins.pug

doctype html
html(class="no-js  page", lang="ru")

  head
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no, user-scalable=no, maximum-scale=1")
    meta(name="robots" content="noindex, nofollow")
    meta(name='description', content='')

    title Страница не найдена

    link(rel="stylesheet", href="css/style.min.css")
    link(rel="shortcut icon", href="/img/favicon.ico", type="image/x-icon")
    script.
      document.documentElement.className = document.documentElement.className.replace('no-js', 'js');

  body
    +page-header("Константин")

    .page-container
      aside.sidebar.pushy.pushy-left#hideAside
        +side-nav()
        +collections()

      main.main
        .site-overlay
        .overlay
        .main__container#container
          .page-not-found
            .page-not-found__inner
              .page-not-found__wrap
                img(src="img/404.png", alt="Страница не найдена" width="228" height="159")

              .page-not-found__info
                span.page-not-found__slogan Упс...
                h1.page-not-found__title Страница не найдена
                p.page-not-found__text
                  | Попробуйте обновить страницу или вернуться назад на главную

            a.page-not-found__btn.btn(href="index.html") На главную

    +page-footer()

    //- модальные окна
    +modal-location()

    +auth-modal()
    +reg-modal()
    +regafter-modal()
    +warnauth-modal()

    script(src="js/jquery.min.js")
    //- script(src="js/jquery-migrate.min.js")
    //- script(src="js/jquery-ui.min.js")
    script(src="https://api-maps.yandex.ru/2.1/?lang=ru_RU")
    script(src="js/bootstrap.bundle.min.js")
    script(src="js/svg4everybody.min.js")
    script(src="js/ofi.min.js")
    script(src="js/pushy.min.js")
    script(src="js/slick.min.js")
    script(src="js/truncate.js")
    script(src="js/auto-search.js")
    script(src="js/jquery.barrating.min.js")
    script(src="js/script.js")
    script(src="js/modals.js")
    script(src="js/map.js")
    //- script(src="js/script.min.js")
